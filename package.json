{
  "name": "toolkit",
  "version": "2.1.0",
  "description": "node-toolkit",
  "main": "./dist/main/src/desktop/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:lint-staged": "lint-staged",
    "lint:eslint": "eslint --fix \"src copy/**/*.{js,jsx,ts,tsx}\"",
    "lint:prettier": "prettier --write \"src copy/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "compile": "tsc",
    "dev": "cross-env NODE_ENV=development webpack server --config ./config/webpack.config.js",
    "webpack": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "dev-electron": "npm run compile && cross-env NODE_ENV=development electron ./dist/main/src/desktop/index.js",
    "prod-electron": "npm run compile && cross-env NODE_ENV=production electron ./dist/main/src/desktop/index.js",
    "build-electron-win64": "npm run compile && electron-builder -w --x64",
    "build": "npm run webpack && npm run build-electron-win64"
  },
  "author": "pzc",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.13",
    "@types/ace": "^0.0.52",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@types/cli-progress": "^3.11.0",
    "@types/jest": "^28.1.1",
    "@types/jscodeshift": "^0.11.5",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.40",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.25",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-loader": "^9.1.3",
    "commitizen": "^4.2.4",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "electron": "^26.6.10",
    "electron-builder": "^24.6.3",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "lint-staged": "^13.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.6.2",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.3",
    "style-resources-loader": "^1.5.0",
    "svg-sprite-loader": "^6.0.11",
    "ts-jest": "^28.0.2",
    "typescript": "^5.4.5",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "sugarss": "^4.0.1",
    "jscodeshift": "^0.13.1",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.0.0",
    "postcss-safe-parser": "^6.0.0",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.4",
    "postcss-syntax": "^0.36.2",
    "posthtml": "^0.16.6",
    "posthtml-parser": "^0.11.0",
    "posthtml-render": "^3.0.0",
    "@vue/compiler-core": "^3.2.36",
    "@vue/compiler-sfc": "^3.2.36",
    "@babel/core": "^7.18.2",
    "@babel/helper-plugin-utils": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.24.6",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@ant-design/icons": "^5.3.7",
    "@reduxjs/toolkit": "^2.2.5",
    "antd": "^5.17.3",
    "brace": "^0.11.1",
    "cli-progress": "^3.12.0",
    "core-js": "^3.31.1",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "github-markdown-css": "^5.5.1",
    "history": "^5.3.0",
    "lru-cache": "^10.2.2",
    "monaco-editor": "^0.44.0",
    "react": "^18.3.1",
    "react-ace": "^11.0.1",
    "react-dom": "^18.3.1",
    "react-intl": "^6.4.4",
    "react-markdown": "^9.0.1",
    "react-monaco-editor": "^0.55.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.15.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "remark-gfm": "^4.0.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ]
  },
  "engines": {
    "node": ">= 10.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "build": {
    "productName": "node-toolkit",
    "appId": "node.toolkit.electron.app",
    "directories": {
      "output": "./dist/win"
    },
    "files": [
      "./package.json",
      "./dist/build/**/*",
      "./dist/main/**/*"
    ],
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./public/electron-logo1.ico",
      "uninstallerIcon": "./public/electron-logo2.ico",
      "installerHeaderIcon": "./public/electron-logo1.png",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "node-toolkit"
    },
    "win": {
      "icon": "./public/electron-logo1.ico",
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "target": "nsis"
    },
    "electronDist": "./electron/electron-v26.6.10-win32-x64"
  }
}
